check process <%= fetch(:private_pub_name) %>
  with pidfile <%= fetch(:private_pub_pid) %>
  start program = "<%= fetch(:private_pub_start) %>" with timeout 90 seconds
  stop program = "kill -s TERM `cat <%= fetch(:private_pub_config) %>`" with timeout 90 seconds
  if totalmem is greater than 200 MB for 2 cycles then restart # eating up memory?
  group <%= fetch(:private_pub_name) %>